{
  "name": "FlipTok",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check-useless-dependencies": "npx depcheck",
    "clean": "find . -name '*.log' -type f -delete && rm -rf node_modules",
    "clean-lockfile": "rm -rf package-lock.json yarn.lock",
    "clean-watchman": "watchman watch-del-all",
    "eslint": "eslint .",
    "eslint-auto-fix-the-whole-project": "eslint . --fix",
    "install-pods": "cd ios && pod install --repo-update",
    "open-android-studio": "open -a /Applications/Android\\ Studio.app android",
    "open-xcode": "xed -b ios",
    "pod-repo-update": "cd ios && pod repo update",
    "prepare": "husky install",
    "prettier-the-whole-project": "prettier --write '**/*.{css,js,json,jsx,markdown,md,scss,ts,tsx}'",
    "reinstall-node-modules": "rm -rf node_modules && yarn",
    "reinstall-pods": "rm -rf ios/build ios/Pods ios/Podfile.lock && yarn install-pods",
    "reset": "yarn run clean && yarn run setup && yarn run install-pods",
    "reset-full": "yarn run clean && yarn run clean-lockfile && yarn run setup",
    "reset-project": "rm -rf android ios && git restore --source=HEAD --staged --worktree -- android && git restore --source=HEAD --staged --worktree -- ios && yarn run reset",
    "run-android": "npx react-native run-android",
    "run-android-production": "npx react-native run-android --variant=release",
    "run-devices": "yarn run-ios-se && yarn run-ios-14-pro-max && yarn run-ios-13 && yarn run-android",
    "run-ios": "npx react-native run-ios",
    "run-ios-15": "npx react-native run-ios --simulator=\"iPhone 15\"",
    "run-ios-15-pro-max": "npx react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
    "run-ios-production": "npx react-native run-ios --configuration Archive",
    "run-ios-real": "npx react-native run-ios --udid=\"00008101-001A48AC0192001E\"",
    "run-ios-se": "npx react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "run-ios-se-production": "npx react-native run-ios --configuration Archive --simulator=\"iPhone SE (3rd generation)\"",
    "setup": "yarn",
    "start": "npx react-native start --reset-cache",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-with-coverage": "jest --coverage",
    "upgrade-all-deps": "npx npm-check-updates --concurrency 999999999 --deep -l silly --timeout 999999999 -u && yarn run setup",
    "upgrade-dev-deps": "npx npm-check-updates --concurrency 999999999 --deep --dep dev -l silly --timeout 999999999 -u && yarn run setup"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-camera-roll/camera-roll": "^7.7.0",
    "@react-native-community/cli": "^12.3.5",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "fbjs": "^3.0.5",
    "husky": "^9.0.11",
    "lottie-react-native": "^6.6.0",
    "pod-install": "^0.2.0",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-blob-util": "^0.19.9",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.7.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-redux": "^9.1.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-json-es": "^1.5.7",
    "eslint-plugin-json-schema-validator": "^4.0.3",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "eslint-plugin-react-redux": "^4.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-this": "^0.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=18"
  }
}
